{
  "name": "shopify-voice-receptionist",
  "version": "1.0.0",
  "description": "Professional voice-first AI receptionist Shopify app using Vapi AI",
  "private": true,
  "author": "Your Company",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "node server.js",
    "dev:http": "next dev --turbopack",
    "dev:https": "node server.js",
    "dev:https:first-time": "npm run gen-certs && npm run dev:https",
    "dev:vercel": "next dev",
    "gen-certs": "node scripts/gen-certs.js",
    "build": "next build",
    "start": "next start",
    "vercel-build": "next build",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,css}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,json,css}'",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist build node_modules",
    "clean:cache": "rm -rf .next"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.20.0",
    "@shopify/app-bridge-react": "^4.2.7",
    "@shopify/polaris": "^13.9.5",
    "@shopify/shopify-api": "^12.0.0",
    "@supabase/supabase-js": "^2.75.1",
    "@vapi-ai/server-sdk": "^0.10.2",
    "date-fns": "^4.1.0",
    "next": "^14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
